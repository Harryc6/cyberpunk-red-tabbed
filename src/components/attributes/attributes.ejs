<div class="section-attributes">
  <div class="columns">
    <% attributes.forEach(function(attribute) { %>
    <div class="column clipped-corner">
      <strong class="accent-font"><%= attribute.name %></strong>
      <input type="number" name="attr_<%= attribute.field %>" />
    </div>
    <% }) %>
  </div>
</div>
